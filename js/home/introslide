  document.querySelector('.productone').style.display = 'none';
  document.querySelector('.productone-b').style.display = 'flex';
});

document.getElementById('dot8').addEventListener('click', function () {
  document.querySelector('.producttwo-ab').style.display = 'none';
  document.querySelector('.producttwo-a').style.display = 'flex';
});


document.getElementById('dot9').addEventListener('click', function () {
  document.querySelector('.producttwo-a').style.display = 'none';
  document.querySelector('.producttwo-ab').style.display = 'flex';
});

